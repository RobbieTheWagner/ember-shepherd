{
  "name": "ember-shepherd",
  "version": "13.0.0",
  "description": "An Ember wrapper for the site tour library Shepherd.",
  "keywords": [
    "ember-addon",
    "site tour",
    "tour",
    "shepherd"
  ],
  "homepage": "http://shipshapecode.github.io/ember-shepherd/",
  "repository": "https://github.com/shipshapecode/ember-shepherd",
  "license": "MIT",
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
    "url": "https://github.com/rwwagner90"
  },
  "contributors": [
    {
      "name": "Alex LaFroscia",
      "email": "alex@lafroscia.com",
      "url": "https://github.com/alexlafroscia"
    },
    {
      "name": "bek",
      "email": "bekzod@me.com",
      "url": "https://github.com/bekzod"
    },
    {
      "name": "Craig Sketchley",
      "email": "craigsketchley@gmail.com",
      "url": "https://github.com/craigsketchley"
    },
    {
      "name": "Chuck Carpenter",
      "email": "",
      "url": "https://github.com/chuckcarpenter"
    },
    {
      "name": "Brian Sipple",
      "email": "",
      "url": "https://github.com/BrianSipple"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "changelog": "github_changelog_generator -u shipshapecode -p ember-shepherd --since-tag v10.0.0 --max-issues 100 --exclude-labels dependencies,duplicate,question,invalid,wontfix",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-auto-import": "^2.4.2",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.0",
    "shepherd.js": "^10.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "ember-cli": "~4.6.0",
    "ember-cli-addon-docs": "^5.0.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-code-coverage": "^2.0.0-beta.4",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.0",
    "ember-cli-stylelint": "^4.0.0",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^4.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-prism": "^0.13.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.6.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.3.4",
    "ember-template-lint": "^4.10.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.4.0",
    "release-it-lerna-changelog": "^5.0.0",
    "sass": "^1.49.0",
    "stylelint": "^14.2.0",
    "stylelint-config-ship-shape": "^0.7.0",
    "webpack": "^5.74.0"
  },
  "resolutions": {
    "**/colors": "1.4.0"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://shipshapecode.github.io/ember-shepherd/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.17"
  }
}
