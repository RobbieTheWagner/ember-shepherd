{
  "name": "ember-shepherd",
  "version": "14.0.2",
  "description": "An Ember wrapper for the site tour library Shepherd.",
  "keywords": [
    "ember-addon",
    "site tour",
    "tour",
    "shepherd"
  ],
  "homepage": "http://RobbieTheWagner.github.io/ember-shepherd/",
  "repository": "https://github.com/RobbieTheWagner/ember-shepherd",
  "license": "MIT",
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
    "url": "https://github.com/RobbieTheWagner"
  },
  "contributors": [
    {
      "name": "Alex LaFroscia",
      "email": "alex@lafroscia.com",
      "url": "https://github.com/alexlafroscia"
    },
    {
      "name": "bek",
      "email": "bekzod@me.com",
      "url": "https://github.com/bekzod"
    },
    {
      "name": "Craig Sketchley",
      "email": "craigsketchley@gmail.com",
      "url": "https://github.com/craigsketchley"
    },
    {
      "name": "Chuck Carpenter",
      "email": "",
      "url": "https://github.com/chuckcarpenter"
    },
    {
      "name": "Brian Sipple",
      "email": "",
      "url": "https://github.com/BrianSipple"
    }
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^8.0.0",
    "ember-cli-htmlbars": "^6.2.0",
    "shepherd.js": "^11.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.19.4",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^3.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "broccoli-asset-rev": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.1",
    "concurrently": "^8.0.1",
    "ember-cli": "~4.12.0",
    "ember-cli-addon-docs": "^6.0.1",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-code-coverage": "^2.0.0-beta.4",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deploy": "^2.0.0",
    "ember-cli-deploy-build": "^3.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.0",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^5.2.0",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-prism": "^0.13.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^11.0.0",
    "ember-source": "~5.5.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-svg-jar": "^2.3.4",
    "ember-template-lint": "^5.7.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.5.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.7",
    "qunit": "^2.19.4",
    "qunit-dom": "^3.0.0",
    "release-it": "^15.4.0",
    "release-it-lerna-changelog": "^5.0.0",
    "sass": "^1.49.0",
    "stylelint": "^15.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-prettier": "^3.0.0",
    "webpack": "^5.78.0"
  },
  "resolutions": {
    "**/colors": "1.4.0"
  },
  "peerDependencies": {
    "ember-source": "^4.0.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://RobbieTheWagner.github.io/ember-shepherd/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.17"
  }
}
